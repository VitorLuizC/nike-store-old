{
  "name": "nike-store",
  "version": "1.0.0",
  "description": "Fictitious Nike store using Stylus, Pug and Webpack.",
  "main": "./index.js",
  "scripts": {
    "clean": "del-cli ./dist",
    "start": "node .",
    "watch": "npm run clean & webpack -d -w --env development",
    "build": "npm run clean & webpack -d --display-error-details --env development",
    "deploy": "npm run clean & webpack -p --env production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VitorLuizC/nike-store.git"
  },
