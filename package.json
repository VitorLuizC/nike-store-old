  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VitorLuizC/nike-store/issues"
  },
  "homepage": "https://github.com/VitorLuizC/nike-store#readme",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.22.0",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "del-cli": "^1.0.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.2.0",
    "pug": "^2.0.0-beta3",
    "pug-loader": "^2.3.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "express": "^4.14.1",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "flickity": "^2.0.5"
  }
